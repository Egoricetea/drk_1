var
  c, d, x: real;

begin
  writeln('Введите X');
  readln(x);
  if x < -7 then
  begin
    c :=ln(x)/ln(10) - (x*x*x);
writeln(c);
end;  
  if x < -7 then
      if x > -5 then
      begin
        c :=exp(1/3*ln(x))/exp(0.1*(x)*ln(x))
end;
  writeln('Такой функции не существует');
  if x >= -5 then
       begin
    c := (ln(x)/(-x))*(sin(x)/(x*x));
writeln(c);
end;
end.